<% if @user %>
    <div class="jumbotron">
        <%= button_to t('.b_back'), accounts_path, method: :get, class:"btn btn-outline-primary my-3", type: 'button' %>
        <h2 id="email" class="display-4">
            <%= @user.email %>
        </h2>

        <%= render 'form', user: @user %>
    </div>
<% end %>